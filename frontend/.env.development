# Example environment configuration - copy to .env and customize
# For local development, you don't need to change anything
# For server deployment, update with your server values

# Environment type
APP_ENV=development
DOCKER=true
SERVER_IP=localhost

# API URL for frontend to connect to backend 
VITE_API_URL=http://localhost:8000

# CORS settings
CORS_ORIGINS=http://localhost:3000

# Database settings
POSTGRES_USER=dev_user
POSTGRES_PASSWORD=dev_password
POSTGRES_DB=app_db